div class='container-fluid' style='margin-top: 10px'
  div class='row'
    - (1..4).each do |index|
      div class='col-xs-12 col-sm-12 col-md-4 col-lg-3'
        div class='thumbnail'
          img src='data:image/svg+xml;base64,' data-holder-rendered='true' class='img-thumbnail' style='display: block'
          div class='caption'
            h3 Name
            p Contact No
            p Location
            p Email
